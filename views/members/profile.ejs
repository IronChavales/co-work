<div class="container">
  <h2> Hello, <%= currentUserInfo.name %> </h2>

  <section>
      <img src="<%= currentUserInfo.pic_path %>" alt="<%= currentUserInfo.pic_name %>" class="circle"/>
  </section>

  <h5>Name:</h5> <p><%= currentUserInfo.name %> </p>
  <h5>E-mail:</h5> <p><%= currentUserInfo.email %> </p>
  <h5>Username:</h5> <p><%= currentUserInfo.username %> </p>

  <a href="/edit-profile/<%= currentUserInfo._id %>" class="waves-effect waves-light btn-large"> Edit my profile </a>

  <a href="/" class="waves-effect waves-light btn-large light-blue darken-2"> Back home </a>

</div>
