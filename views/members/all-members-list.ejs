<nav class="margin-bottom">
  <img src="images/cowork-logo.png" href="/" class="brand-logo">

  <div class="nav-wrapper">
    <ul id="nav-mobile" class="right hide-on-med-and-down">
      <li> <a href="/"> Home </a> </li>
      <li> <a href="/profile"> My profile </a> </li>
      <li> <a href="/logout"> Logout </a> </li>
    </ul>
  </div>
</nav>

<div class="container">

    <h1> All the members</h1>

    <ul class="collection">
    <% for(var i = 0 ; i < members.length; i++ ){%>
       <li class="collection-item">
         <h4><%= members[i].name  %></h4>
         <h5><%= members[i].username %> - <%= members[i].email %></h5>
           <img src="<%= members[i].pic_path %>" alt="<%= members[i].pic_name %>" class="circular circle avatar"/>
           <a href="/profile/<%= members[i]._id %>" class="waves-effect waves-light btn-large right"> See Member</a>

         <% if (currentUserInfo.role === 'Admin') { %>
           <a href="/delete/<%= members[i]._id %>" class="waves-effect waves-light btn-large deep-orange darken-4 right margin-right"> Delete Member</a>
         <% } %>

      </li>
     <% } %>
    </ul>

</div>
